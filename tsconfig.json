{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "app/firebase/firebase.ss", "app/page.jsx", "app/api/likeSong/route.js", "app/api/createUser/route.js", "app/api/createSong/route.js", "app/components/Upload.jsx", "app/components/Sidebar.jsx", "app/components/Allsongs.jsx", "app/components/Player.jsx", "app/components/Search.jsx", "app/components/Liked.jsx"],
  "exclude": ["node_modules"]
}
